a =
  a
    display inline
    text-decoration underline
    // text-decoration-color #fb970077
    text-decoration-color rgba(251, 151, 0, 0.5) !important
    text-decoration-thickness 3px
    text-underline-offset 0.4em
    color black
    font-weight 600
    transition all 0.5s var(--ease-out-expo)

  a:hover
    text-decoration-color rgba(251, 151, 0, 0) !important
    color #6c0b00

:root
  --orange rgb(251, 151, 0)
  --ease-out-expo cubic-bezier(0.16, 1, 0.3, 1)

html.lenis
  height auto

.leftarrow, .rightarrow, .btn, .navbar, .link
  user-select none

.w-nav
  z-index 200

.navbar-wrap--sticky
  .navbar__logo
    color black

  .navbar__menu__link
    color black

  .navbar__btn
    display flex
    padding-bottom 4px

.accordion__text
  display none

.hero__info__title
  z-index 20

.hero__info__titleMark
  z-index 20

.hero__info__title-mark::before
  content url('https:\/\/uploads-ssl.webflow.com\/63eaa604a8a840ce06c9aa6d\/63eb9be563b7cf6636cf97b2_Vectors-Wrapper.svg')
  position absolute
  top 60%
  left 60%
  transform translate(-50%, -50%)
  z-index -1

.what__left::before
  content ''
  display block
  width 240px
  height 100%
  background #FB9700
  position absolute
  top 0
  left 0
  transform translateX(-100%)

html.w-editor::after
  display none !important

// swiper-------------------
.swiper-wrapper
  grid-column-gap 0 !important
  grid-row-gap 0 !important

.consultaion__color-bg__img-bg
  filter drop-shadow(-50px 50px 30px rgba(0, 0, 0, 0.1))

.why__card
  background-color white

.how__swiper, .blog__swiper, .about__blog__swiper
  margin 0 !important
  // width 100% !important

.leftarrow, .rightarrow
  cursor pointer

.consultation__color-bg__img-bg
  filter drop-shadow(-200px 200px 200px rgba(0, 0, 0, 0.5))

.how__container, .blog__container, .about__blog__container
  --swiper-theme-color #fb9700
  --swiper-pagination-bullet-size 10px
  --swiper-pagination-bullet-inactive-color white
  --swiper-pagination-bullet-inactive-opacity 1
  --swiper-pagination-bullet-horizontal-gap 10px

.results__img
  margin 0 !important

.how__container .swiper-wrapper, .about__blog__container .swiper-wrapper
  display block

.results__img
  margin 0 !important

.results__logos__slide
  width auto !important

.why__logos
  width 100%
  height 80px
  overflow visible !important

  .swiper-slide-thumb-active
    // background #000
    filter none
    opacity 1
    transition-duration 0.5s
    transition-property filter, opacity

  .logos__item
    // height auto !important
    width auto
    // max-height 100%
    height 100% !important
    max-width 110px
    // max-height 70px
    object-fit contain

  .results__logos__slide
    cursor pointer
    display flex
    align-items center
    // background #999
    // height 100% !important

.my-career__swiper
  overflow visible !important

.my-career__swiper-slide
  width auto !important

.results
  // width 100%
  // overflow hidden

.webinars__title__swiper
  // .swiper-wrapper
  // display flex
  // gap 60px !important
  .webinars__title
    transition all 0.4s

  .swiper-slide
    cursor pointer

  .swiper-slide:hover
    .webinars__title
      color #fb9700
      transition all 0.4s

  .swiper-slide-thumb-active
    cursor default

    .webinars__title
      color #fb9700

@media screen and (min-width 769px)
  .why__swiper
    // margin 0 !important

    // overflow visible !important

    // width 960px
    // height 280px
    .swiper-wrapper
      display grid
      grid-row-gap 30px !important

  .why__bullets
    display none

  .link--card:hover
    transition all 0.4s

    .card--white-paper__hover
      opacity 1

    .pdf-img
      transform translate(-50%, -70%)
      // top -10px
      transition all 0.4s
      filter drop-shadow(-10px 10px 10px rgba(0, 0, 0, 0.1))

    .white-paper__title
      text-decoration underline
      text-decoration-color #fb9700

    .card--white-paper__img
      background-size auto 110%
      transition all 1.5s

  // 
  // ABOUT
  // 
  .card--blog:hover
    .card__title
      span
        // text-decoration underline
        background-size 0 0.1em, 100% 0.1em
        transition all 2s var(--ease-out-expo)

    .blog-card__text
      opacity 0.8
      transition inherit

    .blog-card__img
      transform scale(1.1)
      transition all 2s var(--ease-out-expo)

  // 
  // RESOURCES
  // 
  .resources-hero__pagination
    bottom 30px !important
    right 30px !important
    top auto !important
    left auto !important

@media screen and (max-width 991px)
  .hero__info__title-mark::before
    transform translate(-50%, -50%) scale(0.7)

  .navbar__menu__link
    color white !important

@media screen and (max-width 768px)
  .hero__info__title-mark::before
    transform translate(-50%, -50%) scale(0.5)

  .how__container .swiper-wrapper, .blog__container .swiper-wrapper, .about__blog__container .swiper-wrapper
    display flex
    align-items start

  .why__swiper
    height 400px

    // padding 5px !important
    .swiper-wrapper
      display flex
      flex-wrap wrap

    .swiper-slide
      height calc(((100% - 40px) / 2)) !important

  .results__img
    width auto
    height auto !important

  .results__img-wrap
    overflow hidden

  .webinars__title_swiper
    .swiper-slide
      // width 120px !important

@media only screen and (max-width 640px)
  .consultaion__img__color-bg, .consultaion__color-bg__img-bg
    // filter drop-shadow(-70px 70px 70px rgba(0, 0, 0, 0.9))
    filter none

  .how__swiper, .blog__swiper, .about__blog__swiper
    overflow hidden !important
    // width 400px !important

@media only screen and (max-width 480px)
  .hero__info__title-mark::before
    transform translate(-50%, -50%) scale(0.35)

  .hero__video
    video
      width auto
      left -30%

  .why__logos
    height 60px
    width 100%
    overflow visible !important

    .logos__item
      height auto
      // width auto
      // max-height 80px
      max-width 120px
      // width 120px
      // height auto

  .testimonials__item__logos
    justify-content space-evenly

  .testimonials__logo
    display flex
    justify-content center

    svg
      max-height 60px
      max-width 80px

  // About ----------------------------------------------------------------
  .my-career__info__year--big::before
    transform translate(-50%, -50%) scale(0.6) !important

.swiper-pagination
  position relative !important

.how__titleNav__arrows
  z-index 100 !important

.text-img__img--career
  clip-path url('#maskCareer')

.text-img__img--success
  clip-path url('#maskSuccess')

.text-img__img--life
  clip-path url('#maskLife')

.w--current
  color #FB9700 !important

// ABOUT ----------------------------------------------------------------
.text-img__info .text
  {a}

.my-career__swiper
  .my-career__info__year--big::before
    display block
    position absolute
    left 50%
    top 50%
    transform translate(-50%, -50%) scale(1)
    z-index -1

  --year-mark-1 url('data:image/svg+xml;base64,')
  --year-mark-2 url('data:image/svg+xml;base64,')
  --year-mark-3 url('data:image/svg+xml;base64,')

  .my-career__info__year--mark1::before
    content var(--year-mark-1)

  .my-career__info__year--mark2::before
    content var(--year-mark-2)

  .my-career__info__year--mark3::before
    content var(--year-mark-3)

// RESOURCES -----------------------
.resources-hero__rightarrow, .resources-hero__leftarrow
  user-select none

.hero__blog__item-wrap:hover
  cursor pointer
  transition all 0.5s

  .hero__blog__title
    color #fb9700

  .btn--resources
    border-color #fb9700 !important
    color #fb9700 !important

.white-paper__swiper
  overflow hidden
  filter drop-shadow(-70px 70px 70px rgba(0, 0, 0, 0.05))

.pdf-img
  transition all 0.4s

// CONTACT ----------------------------------------------------------------
.contact__links
  {a}

// FAQ ----------------------------------------------------------------
.faq .accordion__item:first-child
  border-top-width 0

// BLOG ITEM --------
.card--blog
  .card__title
    span
      // display inline !important
      transition all 2s var(--ease-out-expo) !important
      background linear-gradient(to right, rgba(255, 255, 255, 0.1), rgba(255, 255, 255, 0.1)), linear-gradient(to right, rgba(251, 151, 0, 1), rgba(251, 151, 0, 1))
      background-size 100% 2px, 0 0.1em
      background-position 100% 100%, 0 98%
      background-repeat no-repeat

.blog-item__text, .webinar-item__text
  >*:first-child
    margin-top 0
    padding-top 0

  {a}

  p:has(+figure)
    padding-bottom 0.5em

  figure:has(>div>img)
    // max-width 100%
    // width 100%
    // padding 0 1.2em 1em
    // padding 0 0 1rem
    margin-bottom 1.4rem

  figcaption
    font-size 0.6em

  figure:has(+p)
    padding-bottom 0.5em

  hr
    opacity 0.5
    margin-bottom 2.2rem

  ul>li::marker
    color var(--orange)

  ol>li::marker
    font-weight 600

  ul, ol
    padding-left 2em

    li
      text-indent 0.3em

  ul:has(+p), ol:has(+p)
    opacity 0.5

.blog-item__share, .webinar-item__share
  &::before
    display none

.share__link
  overflow hidden
  transition all 1s var(--ease-out-expo)

  &:hover
    filter invert(63%) sepia(88%) saturate(2327%) hue-rotate(1deg) brightness(101%) contrast(103%)

    .svg, &::after
      transform translateY(-110%)
      transition inherit

.share__link--facebook::after, .share__link--twitter::after, .share__link--linkedin::after
  display block
  position absolute
  bottom -110%
  left 0
  width 100%
  height 100%

.share__link--facebook::after
  content url('data:image/svg+xml;base64,')

.share__link--twitter::after
  content url('data:image/svg+xml;base64,')

.share__link--linkedin::after
  content url('data:image/svg+xml;base64,')

.webinar-video:hover > .play-btn
  background #1F1F1F